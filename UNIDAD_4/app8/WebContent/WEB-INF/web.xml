<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  
   <!-- ================ Servlet Declaraciones ========================= --> 
    <servlet>
        <servlet-name>inicio</servlet-name>
        <servlet-class>com.psp.app.InicioServlet</servlet-class>    
    </servlet>
    
     <servlet>
        <servlet-name>saludo</servlet-name>
        <servlet-class>com.psp.app.SaludoServlet</servlet-class>    
    </servlet>
  <!-- ================ Servlet Mappings ========================= -->
    <servlet-mapping>
        <servlet-name>inicio</servlet-name>
        <url-pattern>/inicio</url-pattern>
    </servlet-mapping>
    
     <servlet-mapping>
        <servlet-name>saludo</servlet-name>
        <url-pattern>/saludo</url-pattern>
    </servlet-mapping>
    
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>